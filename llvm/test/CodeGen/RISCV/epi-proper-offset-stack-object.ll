; CHECK-NEXT:    vse64.v v16, (a0)
; CHECK-NEXT:    addi sp, sp, 1040
; CHECK-NEXT:    ret
entry:
  %mask_le.i = alloca %union.anon, align 1
  %test_mask.i = alloca %union.anon.0, align 8
